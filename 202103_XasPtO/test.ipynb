    "import numpy\n",
    "a=[\n",
    "    numpy.array([1,2,3]),\n",
    "    numpy.array([[1],[2],[3]])\n",
    "    ]\n",
    "int_lenline = numpy.shape( a[0] )[0]\n",
    "for int_i in range(len(a)):\n",
    "    if ( a[int_i].ndim == 2 ): continue\n",
    "    a[int_i] = a[int_i].reshape(int_lenline,1)\n",
    "print(a)"
